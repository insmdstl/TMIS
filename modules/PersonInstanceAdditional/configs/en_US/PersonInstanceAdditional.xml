<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonInstanceAdditional" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="PersonInstanceAdditional" path="/I2CE" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="person_can_view_child_form_person_instance">
        <value>Can view person_instance child form of a person</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_instance">
        <value>Can edit person_instance child forms of a person</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
        <configurationGroup name="iHRIS_PersonInstance">
          <configurationGroup name="fields">
		  
            <configurationGroup name="certificate">
              <configuration name="headers" locale="en_US" type="delimited">
                <value>default:Training Certificate</value>
              </configuration>
            </configurationGroup>
			
			<configurationGroup name="fuat_result">
              <configuration name="headers" locale="en_US" type="delimited">
                <value>default:FUAT Result</value>
              </configuration>
            </configurationGroup>
			
			<configurationGroup name="fuat_date">
              <configuration name="headers" locale="en_US" type="delimited">
                <value>default:FUAT Date</value>
              </configuration>
            </configurationGroup>
			
			<configurationGroup name="fuat_document">
              <configuration name="headers" locale="en_US" type="delimited">
                <value>default:FUAT Document</value>
              </configuration>
            </configurationGroup>
			
			<configurationGroup name="fuat_point">
              <configuration name="headers" locale="en_US" type="delimited">
                <value>default:FUAT Point</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>