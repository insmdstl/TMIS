<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainingModules" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="TrainingModules" path="/I2CE" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="can_edit_database_list_training_modules" locale="en_US">
        <value>Edit the Training Modules list</value>
      </configuration>
      <configuration name="can_view_database_list_training_modules" locale="en_US">
        <value>View the Training Modules list</value>
      </configuration>
    </configurationGroup>
      
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms"> 
      </configurationGroup>
	  
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_TrainingModules">
          <configurationGroup name="fields">
            			
            <configurationGroup name="person_responsible">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Person in Charge</value>
              </configuration>
            </configurationGroup>
            
			<configurationGroup name="training_curriculum">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Training Curriculum</value>
              </configuration>
            </configurationGroup>
			
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
</I2CEConfiguration>